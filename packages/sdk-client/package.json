  },
  "dependencies": {
    "node-fetch": "2.7.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.6",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
