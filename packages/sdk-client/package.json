{
  "name": "@sinch/sdk-client",
  "version": "1.1.0",
  "description": "Core services related to interacting with Sinch API",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "Apache-2.0",
  "author": "Sinch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
